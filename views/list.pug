extends layout

block content
    table(cellpadding="0" cellspacing="0")
        thead
            tr
                th id
                th 账号
                th 登录时间
                th 登录IP
        tbody        
            -for (var i=0, l =datas.length; i<l; i++)
                tr
                    td #{datas[i]._id}
                    td #{datas[i].userName}
                    td #{datas[i].lastLoginDate.toLocaleString()}
                    td #{datas[i].IP}